 = data.results[0].url\n      this.setState({footerDate: date})\n      this.setState({firstArticleImg: firstArticleImg})\n      this.setState({firstArticleTitle: firstArticleTitle})\n      this.setState({firstArticleAbstract: firstArticleAbstract})\n      this.setState({firstArticleDate: firstArticleDate})\n      this.setState({firstArticleUrl: firstArticleUrl})\n\n      return (\n        <div id='article' className='article' key={i}>\n          <a className='minipic'><img\n            className='thumbnail'\n            alt=''\n            src={e.thumbnail_standard}>\n          </img></a>< br/>\n          <span className='listSpan'>\n            <a\n              className='title'\n              href={e.url} rel=\"noreferrer\"\n              target=\"_blank\">{e.title}\n            </a>< br/>< br/>\n            {e.abstract}< br/>< br/>\n            Published: {e.first_published_date.split('T')[0]} at {e.first_published_date.split('T')[1].split('-')[0]}\n          </span>\n        </div>\n        )\n    })}))\n  }\n\n  render() {\n    const windowWidth = window.innerWidth\n    return (\n      <div className='App'>\n        <div className='banner'>\n          <span className='nytLogo'></span>\n        </div>\n        <div id='containerDiv' className='container'>\n          <div className='firstArticle'>\n            <img className='firstThumbnail' alt=''\n            src={this.state.firstArticleImg}></img>< br/>\n            <span className='firstArticleSpan'>\n              <a className='title' href={this.state.firstArticleUrl}\n              rel='noreferrer' target='_blank'>{this.state.firstArticleTitle}\n              </a>< br/>< br/>\n              {this.state.firstArticleAbstract}< br/>< br/>\n              {this.state.firstArticleDate}\n            </span>\n          </div>\n            {windowWidth > 1240 ? (\n              <div id='articleList'\n                className='list'>{this.state.article\n                  .slice(1)\n                  .reverse()\n                  .slice(13)\n                  .reverse()}\n              </div>\n            ) : (\n              <div\n                id='articleList'\n                className='list'>{this.state.article\n                  .slice(1)\n                  .reverse()\n                  .slice(16)\n                  .reverse()}\n            </div>)}\n          </div>\n           <div className='footer'>\n           <div className='date'>{this.state.footerDate}</div>\n          <Button\n            className='subscribe'\n            text='Subscribe'\n            handleClick={this.subscribe} />\n          <Button\n            className='digitalEdition'\n            text='Digital Edition'\n            handleClick={this.digitalEdition} />\n          <Button\n            className='archives'\n            text='Archives'\n            handleClick={this.archives} />\n          <Button\n            className='renewal'\n            text='Renewal'\n            handleClick={this.renewal} />\n          </div>\n      </div>\n    )\n  }\n}\n\nexport default App;\n","/Users/tuerkbot/Desktop/dawn-of-the-NYTimes/src/reportWebVitals.js",[],"/Users/tuerkbot/Desktop/dawn-of-the-NYTimes/src/Button.js",[],["41","42"],"/Users/tuerkbot/Desktop/cypress/src/index.js",[],"/Users/tuerkbot/Desktop/cypress/src/reportWebVitals.js",[],"/Users/tuerkbot/Desktop/cypress/src/App.js",["43"],"/Users/tuerkbot/Desktop/cypress/src/Button.js",[],{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","severity":1,"message":"49","line":62,"column":11,"nodeType":"50","endLine":62,"endColumn":34},{"ruleId":"44","replacedBy":"51"},{"ruleId":"46","replacedBy":"52"},{"ruleId":"48","severity":1,"message":"49","line":62,"column":11,"nodeType":"50","endLine":62,"endColumn":34},"no-native-reassign",["53"],"no-negated-in-lhs",["54"],"jsx-a11y/anchor-is-valid","The href attribute is required for an anchor to be keyboard accessible. Provide a valid, navigable address as the href value. If you canno